# See https://docs.hoarder.app/configuration for more information
DATA_DIR=<path>
NEXTAUTH_SECRET=<secret>

# Google OAuth Configuration
GOOGLE_CLIENT_ID=<your-google-client-id>
GOOGLE_CLIENT_SECRET=<your-google-client-secret>

# GitHub OAuth Configuration
GITHUB_ID=<your-github-client-id>
GITHUB_SECRET=<your-github-client-secret>

# Microsoft OAuth Configuration
MICROSOFT_ID=<your-microsoft-client-id>
MICROSOFT_SECRET=<your-microsoft-client-secret>
MICROSOFT_TENANT_ID=<your-microsoft-tenant-id>  # Optional, defaults to 'common'

# Twitter OAuth Configuration
TWITTER_ID=<your-twitter-client-id>
TWITTER_SECRET=<your-twitter-client-secret>

# Zhipu AI Configuration
ZHIPU_API_KEY=<your-zhipu-api-key>
ZHIPU_BASE_URL=<your-zhipu-base-url>  # Optional, defaults to https://open.bigmodel.cn/api/paas/v3